<VirtualHost *:80>
    DocumentRoot /projects/api/web
    ServerName api.sportlobster.dev

    SetEnv env dev
    <Directory "/projects/api/web">
        Options Indexes FollowSymLinks Includes ExecCGI
        AllowOverride All
        Require all granted
    </Directory>
</VirtualHost>
<VirtualHost *:80>
    DocumentRoot /projects/api/web
    ServerName api.sportlobster.test

    SetEnv env test
    <Directory "/projects/api/web">
        Options Indexes FollowSymLinks Includes ExecCGI
        AllowOverride All
        Require all granted
    </Directory>
</VirtualHost>